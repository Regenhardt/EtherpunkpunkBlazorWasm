  "classifications": [ "blazorwasm", "Web", "Blazor" ],
  "name": "Etherpunk's Blazor Wasm",
  "identity": "Etherpunk.BlazorWasm.CSharp",
  "groupIdentity": "Etherpunk.BlazorWasm",
  "shortName": "etherpunkwasm",
