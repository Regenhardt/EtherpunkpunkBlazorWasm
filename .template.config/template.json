  "sourceName": "EtherpunkBlazorWasm",
  "preferNameDirectory": true,

  "tags": {
    "language": "C#",
    "type": "project"
  },
  "guids": [
    "4185ac7a-3ee6-4060-a3e8-95562d509c52"
