  "symbols": {
    "appDbContextName": {
      "type": "parameter",
      "defaultValue": "DbContext",
      "replaces": "EpunkDbContext"
    },
    "copyrightName": {
      "type": "parameter",
      "displayName": "Copyright String (e.g. (C) 2022 Etherpunk)",
      "defaultValue": "Etherpunk",
      "replaces": "EpunkCopyright"
    },
    "databaseFileName": {
      "type": "parameter",
      "displayName": "Name of the database file for SQLite",
      "defaultValue": "database.sqlite3",
