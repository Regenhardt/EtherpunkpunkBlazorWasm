    },
    "etherpunkAudience": {
      "type": "parameter",
      "displayName": "Audience (used for JWT token)",
      "defaultValue": "EtherpunkAud",
      "replaces": "EpunkAud"
    },
    "etherpunkIssuer": {
      "type": "parameter",
      "displayName": "Issuer (used for JWT token)",
      "defaultValue": "EtherpunkIss",
      "replaces": "EpunkIss"
    },
    "passwordSalt": {
      "type": "parameter",
      "displayName": "Salt to be used. MUST be 22 characters. !! CHANGE THIS FROM DEFAULT !!",
      "defaultValue": "k87L/MF28Q673VKh8/cPi.",
